{
	"luci-app-qbittorrent": {
		"description": "Grant UCI access for luci-app-qbittorrent",
		"read": {
			"ubus": {
				"service": [ "list" ],
				"luci": [ "setInitAction" ]
			},
			"file": {
				"/etc/passwd": [ "read" ],
				"/usr/bin/qbittorrent-nox": [ "exec" ],
				"/sbin/logread": [ "exec" ],
				"/usr/libexec/qbittorrent-call": [ "exec" ]
			},
			"uci": [ "qbittorrent" ]
		},
		"write": {
			"uci": [ "qbittorrent" ]
		}
	}
}
